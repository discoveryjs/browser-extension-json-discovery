{
    "version": null,
    "name": "JsonDiscovery",
    "short_name": "JsonDiscovery",
    "description": "Browser extension that changes the way you\\'re viewing JSON",
    "author": "exdis",
    "manifest_version": 2,
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "permissions": [
        "<all_urls>",
        "storage"
    ],
    "content_scripts": [{
        "js": ["loader.js"],
        "run_at": "document_start",
        "matches": ["<all_urls>"]
    }],
    "web_accessible_resources": [
        "index.css",
        "loader.css",
        "init-discovery.js",
        "icons/*"
    ],
    "offline_enabled": true
}
